    "expo-linear-gradient": "^13.0.2",
    "expo-location": "^17.0.1",
    "expo-status-bar": "~1.11.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.73.6",
    "react-native-calendars": "^1.1304.1",
